    "eslint": "~1.x",
    "eslint-config-airbnb": "0.0.9",
    "eslint-plugin-react": "~3.2.2",
    "extract-text-webpack-plugin": "~0.8.2",
    "less": "~2.5.1",
    "mocha": "~2.2.5",
    "precommit-hook": "~3.0.0",
